#!/usr/bin/python

import serial
import sys

ser = serial.Serial (
                     port=sys.argv[1],
                     baudrate=115200,
                     bytesize=serial.EIGHTBITS,
                     parity=serial.PARITY_NONE,
                     stopbits=serial.STOPBITS_ONE,
                     timeout=0
)

line = '348f27af1372339a'
line_stripped = line.strip()
ser.write(line_stripped.decode("hex"))

ser.close()
